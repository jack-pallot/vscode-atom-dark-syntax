{
	"type": "dark",
	"colors": {
		"editor.background": "#1D1F21",
		"editor.foreground": "#C5C8C6",
		"editor.selectionBackground": "#444444",
		"editor.lineHighlightBackground": "#3A3A3A",
		"editorCursor.foreground": "#A5A2A2",
		"editorWhitespace.foreground": "#C5C8C6",
